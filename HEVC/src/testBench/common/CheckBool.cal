package testBench.common;

import  testBench.common.SourceUnit.*;

actor CheckBool(String portName)
	bool RealValue,
	bool ValueGenerated
		==>
		:

int numToken;

	action RealValue:[r], ValueGenerated:[v] ==> 
	do
		if(v!=r) then
			println("Port "+portName+" : "+v+" instead of "+r + " @ " + numToken);
			source_exit(-1);
		end
		numToken := numToken + 1;
	end
end
package testBench.common;

import  testBench.common.SourceUnit.*;

actor CheckBool(String portName)
	bool RealValue,
	bool ValueGenerated
		==>
		:

	action RealValue:[r], ValueGenerated:[v] ==> 
	do
		if(v!=r) then
			println("Port "+portName+" : "+v+" instead of "+r);
			source_exit(-1);
		end
	end
end
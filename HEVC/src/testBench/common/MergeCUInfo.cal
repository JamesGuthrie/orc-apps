package testBench.common;


actor MergeCUInfo()
	uint(size=8)  CUMode,
	uint(size=16) CUAddr,
	uint(size=7)  CUSize
		==>
	uint(size=16) CUInfo
		:


	action CUSize:[cuSize] repeat 2, CUAddr:[cuAddr] repeat 2, CUMode:[cuMode]  ==>
		CUInfo:[cuMode,cuAddr[0],cuAddr[1],cuSize[0],cuSize[1]]
	end
end
package zbug;

import zbug.Common.* ;

actor HeaderParser () uint(size=8) Stream ==>  uint(size=8) O :


	/*
	 *  bug : SyncByte is initialize by null
	 *  into class AstTransformer line 1201 (wintin transformGlobalVariable method)
	 */
	Sync.Done: action Stream:[ sync ] ==>
	guard
		sync = SyncByte
	do
		println("Sync ok!");
	end

	Sync.Error: action Stream:[ sync ] ==>
	do
		println("Sync error: " + sync);
	end

	priority
		Sync.Done > Sync.Error;
	end

end
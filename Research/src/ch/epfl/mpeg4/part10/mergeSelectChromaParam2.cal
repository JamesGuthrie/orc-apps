
package ch.epfl.mpeg4.part10;

import std.util.Math.*;

actor mergeSelectChromaParam2()
	uint (size=8)  CoeffAcrU,
	uint (size=8)  CoeffAcrV
		==>
	uint (size=8)  CoeffAcr
		:

	action
			CoeffAcrU:[x] repeat 64,
			CoeffAcrV:[y] repeat 64
				==>
			CoeffAcr:[z] repeat 128
	var
		uint(size=8) z[128]
	do
		foreach int i in 0 .. 63
		do
			z[i] := x[i];
			z[i+64] := y[i];
		end
	end

end
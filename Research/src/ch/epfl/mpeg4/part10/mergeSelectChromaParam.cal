
package ch.epfl.mpeg4.part10;

import std.util.Math.*;

actor mergeSelectChromaParam()
	int (size=16)  CoeffAcrU,
	int (size=16)  CoeffAcrV
		==>
	int (size=16)  CoeffAcr
		:

	action
			CoeffAcrU:[x] repeat 64,
			CoeffAcrV:[y] repeat 64
				==>
			CoeffAcr:[z] repeat 128
	var
		int(size=16) z[128]
	do
		foreach int i in 0 .. 63
		do
			z[i] := x[i];
			z[i+64] := y[i];
		end
	end

end

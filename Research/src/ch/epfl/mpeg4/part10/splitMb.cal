
package ch.epfl.mpeg4.part10;

import std.util.Math.*;

actor splitMb()
	uint(size=8)  MbIn
		==>
	uint(size=8)  MbOutU,
	uint(size=8)  MbOutV
		:

	action
		MbIn:[x] repeat 128
			==>
		MbOutU:[[x[i]:for int i in 0 .. 63]] repeat 64,
		MbOutV:[[x[i]:for int i in 64 .. 127]] repeat 64
	end

end

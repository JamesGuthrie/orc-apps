package ch.epfl.mpeg4.part10.svc.synParser.modular.decodePic;

import ch.epfl.mpeg4.part10.cbp.synParser.modular.decodePicture.decodingFunctions.*;

actor MgntLayer()
	uint(size=5)  NalUnitType,
	int (size=32) ValVLD,

	int (size=6)  Header1AvcTypeOfVLD,
	int (size=32) Header1AvcVLDParam,
	int (size=6)  Header1SvcTypeOfVLD,
	int (size=32) Header1SvcVLDParam,
	int (size=6)  Header2AvcTypeOfVLD,
	int (size=32) Header2AvcVLDParam,
	int (size=6)  Header2SvcTypeOfVLD,
	int (size=32) Header2SvcVLDParam,
	uint(size=5)  DecRefPicMarkingTypeOfVLD,
	int (size=32) DecRefPicMarkingNbBitsToRead,
	uint(size=5)  DecRefPicMarkingSvcTypeOfVLD,
	int (size=32) DecRefPicMarkingSvcNbBitsToRead,
	uint(size=5)  DecRefBsPicMarkTypeOfVLD,
	int (size=32) DecRefBsPicMarkNbBitsToRead,
	uint(size=5)  RefPicListTypeOfVLD,
	int (size=32) RefPicListNbBitsToRead,
	int (size=6)  DataAvcTypeOfVLD,
	int (size=32) DataAvcVLDParam,
	int (size=6)  DataSvcTypeOfVLD,
	int (size=32) DataSvcVLDParam,
	int (size=6)  MbLayerAvcTypeOfVLD,
	int (size=32) MbLayerAvcVLDParam,
	int (size=6)  MbLayerSvcTypeOfVLD,
	int (size=32) MbLayerSvcVLDParam,
	uint(size=5)  MbPredTypeOfVLD,
	int (size=32) MbPredNbBitsToRead,
	uint(size=5)  SubMbPredTypeOfVLD,
	int (size=32) SubMbPredNbBitsToRead,
	int (size=6)  ResTypeOfVLD,
	int (size=32) ResActorLaunched,
	uint(size=5)  ResBlkCavlcTypeOfVLD,
	int (size=32) ResBlkCavlcVLD_Param,
	int (size=6)  PrefixTypeOfVLD,
	int (size=32) PrefixVLD_Param,
	uint(size=2)  NalRefIdc
		==>
	uint(size=5)  TypeOfVLD,
	int (size=32) VLD_Param,
	int (size=32) Header1AvcValVLD,
	int (size=32) Header1SvcValVLD,
	int (size=32) Header2AvcValVLD,
	int (size=32) Header2SvcValVLD,
	int (size=32) DecRefPicMarkingValVLD,
	int (size=32) DecRefPicMarkingSvcValVLD,
	int (size=32) DecRefBsPicMarkValVLD,
	int (size=32) RefPicListValVLD,
	int (size=32) DataAvcValVLD,
	int (size=32) DataSvcValVLD,
	int (size=32) MbLayerAvcValVLD,
	int (size=32) MbLayerSvcValVLD,
	int (size=32) MbPredValVLD,
	int (size=32) SubMbPredValVLD,
	int (size=32) ResBlkCavlcValVLD,
	int (size=32) PrefixValVLD,
	uint(size=2)  NalRefIdcAvcHead,
	uint(size=2)  NalRefIdcSvcHead,
	uint(size=2)  NalRefIdcPrefix
		:

	uint(size=5) NAL_PREFIX_NAL    = 14;
	uint(size=5) NAL_SVC_SLICE     = 20;

	bool          isSvc;
	uint(size=8)  callStack[16];
	uint(size=32) nbParamToDecod := 0;
	int (size=4)  stackIdx;

	getNewSlice.isAvcSlice: action NalUnitType:[nalUnitType],
		NalRefIdc:[nalRefIdc] ==> NalRefIdcAvcHead:[nalRefIdc]
	guard
		nalUnitType != NAL_PREFIX_NAL and nalUnitType != NAL_SVC_SLICE
	do
		isSvc        := false;
		stackIdx     := 0;
		callStack[0] := SLICE_HEADER1;
	end

	getNewSlice.isSvcSlice: action NalUnitType:[nalUnitType],
		NalRefIdc:[nalRefIdc] ==> NalRefIdcSvcHead:[nalRefIdc]
	guard
		nalUnitType = NAL_SVC_SLICE
	do
		isSvc        := true;
		stackIdx     := 0;
		callStack[0] := SLICE_HEADER1;
	end

	getNewSlice.isPrefixNal: action NalUnitType:[nalUnitType],
		NalRefIdc:[nalRefIdc] ==> NalRefIdcPrefix:[nalRefIdc]
	guard
		nalUnitType = NAL_PREFIX_NAL
	do
		stackIdx     := 0;
		callStack[0] := PREFIX_NAL_UNIT;
	end

	stackIsEmpty: action ==> TypeOfVLD:[SKIP_NAL]
	guard
		stackIdx < 0
	end

	findOutNextActor.isSliceHeader1Avc: action ==>
	guard
		callStack[stackIdx] = SLICE_HEADER1 and not isSvc
	end

/*	findOutNextActor.isSliceHeader1Svc: action ==>
	guard
		callStack[stackIdx] = SLICE_HEADER1 and isSvc
	end*/

	findOutNextActor.isSliceHeader2Avc: action ==>
	guard
		callStack[stackIdx] = SLICE_HEADER2 and not isSvc
	end

	findOutNextActor.isSliceHeader2Svc: action ==>
	guard
		callStack[stackIdx] = SLICE_HEADER2 and isSvc
	end

	findOutNextActor.isRefPicMark: action ==>
	guard
		callStack[stackIdx] = REF_PIC_MARKING
	end

	findOutNextActor.isRefBasePicMark: action ==>
	guard
		callStack[stackIdx] = REF_BASE_PIC_MARKING
	end

	findOutNextActor.isRefPicList: action ==>
	guard
		callStack[stackIdx] = REF_PIC_LIST
	end

	findOutNextActor.isSliceDataAvc: action ==>
	guard
		callStack[stackIdx] = SLICE_DATA
	end

	findOutNextActor.isSliceDataSvc: action ==>
	guard
		callStack[stackIdx] = SLICE_DATA_SVC
	end

	findOutNextActor.isMbLayerAvc: action ==>
	guard
		callStack[stackIdx] = MB_LAYER
	end

	findOutNextActor.isMbLayerSvc: action ==>
	guard
		callStack[stackIdx] = MB_LAYER_SVC
	end

	findOutNextActor.isMbPred: action ==>
	guard
		callStack[stackIdx] = MB_PRED
	end

	findOutNextActor.isSubMbPred: action ==>
	guard
		callStack[stackIdx] = SUB_MB_PRED
	end

	findOutNextActor.isResidual: action ==>
	guard
		callStack[stackIdx] = RESIDUAL
	end

	findOutNextActor.isResidualCavlc: action ==>
	guard
		callStack[stackIdx] = RESIDUAL_BLOCK_CAVLC
	end

	findOutNextActor.isPrefix: action ==>
	guard
		callStack[stackIdx] = PREFIX_NAL_UNIT
	end

	SliceHeader1Avc.GetVLDVal.Launch: action ValVLD :[VLDecoding] ==>
		Header1AvcValVLD :[VLDecoding]
	guard
		nbParamToDecod > 0
	do
		nbParamToDecod := nbParamToDecod - 1;
	end

	SliceHeader1Avc.GetVLDVal.Done: action ==>
	guard
		nbParamToDecod = 0
	end

	SliceHeader1Avc.WaitVLDVal: action ==>
	guard
		nbParamToDecod > 0
	end

	SliceHeader1Avc.FunctionWithoutParam: action Header1AvcTypeOfVLD :[ValTypeOfVLD] ==>
		TypeOfVLD :[ValTypeOfVLD]
	guard
		NbParamForFunction(ValTypeOfVLD) = 0,
		ValTypeOfVLD  != SKIP_NAL
	do
		if(ValTypeOfVLD != BYTE_ALIGNED_REQUIRED)then
			nbParamToDecod := nbParamToDecod + 1;
		end
	end

	SliceHeader1Avc.FunctionWith1Param: action
		Header1AvcTypeOfVLD :[ValTypeOfVLD],
		Header1AvcVLDParam  :[Param]
			==>
		TypeOfVLD       :[ValTypeOfVLD],
		VLD_Param       :[Param]
	guard
		NbParamForFunction(ValTypeOfVLD) = 1
	do
		nbParamToDecod := nbParamToDecod + 1;
	end

	SliceHeader1Avc.LaunchActor: action
		Header1AvcTypeOfVLD :[Function],
		Header1AvcVLDParam  :[Actor]
			==>
	guard
		Function = LAUNCH_ACTOR
	do
		stackIdx       := (stackIdx + 1) &0x0F;
		callStack[stackIdx] := Actor;
	end

	SliceHeader1Avc.Done: action Header1AvcTypeOfVLD :[Function] ==>
	guard
		Function = SKIP_NAL
	do
		stackIdx := stackIdx - 1;
	end

	SlcHead2Avc.GetVLDVal.Launch: action ValVLD :[VLDecoding] ==>
		Header2AvcValVLD :[VLDecoding]
	guard
		nbParamToDecod > 0
	do
		nbParamToDecod := nbParamToDecod - 1;
	end

	SlcHead2Avc.GetVLDVal.Done: action ==>
	guard
		nbParamToDecod = 0
	end

	SlcHead2Avc.WaitVLDVal: action ==>
	guard
		nbParamToDecod > 0
	end

	SlcHead2Avc.FunctionWithoutParam: action Header2AvcTypeOfVLD :[ValTypeOfVLD] ==>
		TypeOfVLD :[ValTypeOfVLD]
	guard
		NbParamForFunction(ValTypeOfVLD) = 0,
		ValTypeOfVLD  != SKIP_NAL
	do
		if(ValTypeOfVLD != BYTE_ALIGNED_REQUIRED)then
			nbParamToDecod := nbParamToDecod + 1;
		end
	end

	SlcHead2Avc.FunctionWith1Param: action
		Header2AvcTypeOfVLD :[ValTypeOfVLD],
		Header2AvcVLDParam  :[Param]
			==>
		TypeOfVLD       :[ValTypeOfVLD],
		VLD_Param       :[Param]
	guard
		NbParamForFunction(ValTypeOfVLD) = 1
	do
		nbParamToDecod := nbParamToDecod + 1;
	end

	SlcHead2Avc.LaunchActor: action
		Header2AvcTypeOfVLD :[Function],
		Header2AvcVLDParam  :[Actor]
			==>
	guard
		Function = LAUNCH_ACTOR
	do
		stackIdx := (stackIdx + 1) & 0x0F;
		callStack[stackIdx] := Actor;
	end

	SlcHead2Avc.Done: action Header2AvcTypeOfVLD :[Function] ==>
	guard
		Function = SKIP_NAL
	do
		stackIdx := (stackIdx - 1) & 0x0F;
	end

	SlcHead2Svc.GetVLDVal.Launch: action ValVLD :[VLDecoding] ==>
		Header2SvcValVLD :[VLDecoding]
	guard
		nbParamToDecod > 0
	do
		nbParamToDecod := nbParamToDecod - 1;

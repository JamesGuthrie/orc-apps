	end

	RefPicMark.Done: action DecRefPicMarkingTypeOfVLD :[Function] ==>
	guard
		Function = SKIP_NAL
	do
		stackIdx := stackIdx - 1;
	end

	RefBasePicMark.GetVLDVal.Launch: action

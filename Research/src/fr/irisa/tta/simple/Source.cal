package fr.irisa.tta.simple;

import fr.irisa.tta.data.example.*;

actor Source () ==> uint(size=8) O :

	int i := 0;

	sendData: action ==> O:[ Out ]
	guard
		i < INPUT_SIZE
	var
		uint(size=8) Out
	do
		Out := INPUT[i];
		i := i+1;
	end

end

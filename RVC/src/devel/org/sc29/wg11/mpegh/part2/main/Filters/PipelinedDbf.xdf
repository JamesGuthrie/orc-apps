    </Port>
    <Port kind="Input" name="Qp">
        <Type name="int">
            <Entry kind="Expr" name="size">
                <Expr kind="Literal" literal-kind="Integer" value="8"/>
            </Entry>
        </Type>
    </Port>
    <Port kind="Input" name="IsPicSlcLcu">
        <Type name="uint">
            <Entry kind="Expr" name="size">
                <Expr kind="Literal" literal-kind="Integer" value="2"/>
            </Entry>
        </Type>
    </Port>
    <Port kind="Input" name="DBFDisable">
        <Type name="bool"/>
    </Port>
    <Port kind="Input" name="DbfSe">
        <Type name="int">
            <Entry kind="Expr" name="size">
                <Expr kind="Literal" literal-kind="Integer" value="8"/>
            </Entry>
        </Type>
    </Port>
    <Port kind="Output" name="FiltSample">
        <Type name="uint">
            <Entry kind="Expr" name="size">
                <Expr kind="Literal" literal-kind="Integer" value="8"/>
            </Entry>
        </Type>
    </Port>
    <Instance id="DeblockFiltVert">
        <Class name="devel.org.sc29.wg11.mpegh.part2.main.Filters.DeblockingFilter"/>
        <Parameter name="FILT_HOR_EDGES">
            <Expr kind="Literal" literal-kind="Boolean" value="true"/>
        </Parameter>
        <Parameter name="FILT_VERT_EDGES">
            <Expr kind="Literal" literal-kind="Boolean" value="true"/>
        </Parameter>
        <Parameter name="DEBUG">
            <Expr kind="Literal" literal-kind="Boolean" value="false"/>
        </Parameter>
    </Instance>
    <Instance id="GenerateBs">
        <Class name="devel.org.sc29.wg11.mpegh.part2.main.Filters.GenerateBs"/>
    </Instance>
    <Instance id="DeblockFiltHoriz">
        <Class name="devel.org.sc29.wg11.mpegh.part2.main.Filters.DeblockingFilter"/>
        <Parameter name="DEBUG">
            <Expr kind="Literal" literal-kind="Boolean" value="false"/>
        </Parameter>
        <Parameter name="FILT_HOR_EDGES">
            <Expr kind="Literal" literal-kind="Boolean" value="true"/>
        </Parameter>
        <Parameter name="FILT_VERT_EDGES">
            <Expr kind="Literal" literal-kind="Boolean" value="true"/>
        </Parameter>
    </Instance>
    <Connection dst="DeblockFiltVert" dst-port="Bs" src="GenerateBs" src-port="BS"/>
    <Connection dst="DeblockFiltVert" dst-port="PicSize" src="" src-port="PicSize"/>
    <Connection dst="DeblockFiltVert" dst-port="Qp" src="" src-port="Qp"/>
    <Connection dst="DeblockFiltVert" dst-port="SampleIn" src="" src-port="SampleIn"/>
    <Connection dst="GenerateBs" dst-port="CUInfo" src="" src-port="CuInfo"/>
    <Connection dst="GenerateBs" dst-port="Cbf" src="" src-port="Cbf"/>
    <Connection dst="GenerateBs" dst-port="IsBiPredOrLx" src="" src-port="IsBiPredOrLx"/>
    <Connection dst="GenerateBs" dst-port="Mv" src="" src-port="Mv"/>
    <Connection dst="GenerateBs" dst-port="SplitTransf" src="" src-port="SplitTransf"/>
    <Connection dst="GenerateBs" dst-port="PartMode" src="" src-port="PartMode"/>
    <Connection dst="GenerateBs" dst-port="RefPoc" src="" src-port="RefPoc"/>
    <Connection dst="GenerateBs" dst-port="SliceAddr" src="" src-port="SlcAddr"/>
    <Connection dst="DeblockFiltVert" dst-port="IsPicSlcLcu" src="" src-port="IsPicSlcLcu"/>
    <Connection dst="DeblockFiltVert" dst-port="DBFDisable" src="" src-port="DBFDisable"/>
    <Connection dst="GenerateBs" dst-port="LFAcrossSlcTile" src="" src-port="LFAcrossSlcTile"/>
    <Connection dst="GenerateBs" dst-port="LcuSizeMax" src="" src-port="LcuSizeMax"/>
    <Connection dst="DeblockFiltVert" dst-port="LcuSizeMax" src="" src-port="LcuSizeMax"/>
    <Connection dst="GenerateBs" dst-port="TilesCoord" src="" src-port="TilesCoord"/>
    <Connection dst="DeblockFiltVert" dst-port="TilesCoord" src="" src-port="TilesCoord"/>
    <Connection dst="DeblockFiltVert" dst-port="SyntaxElem" src="" src-port="DbfSe"/>
    <Connection dst="DeblockFiltHoriz" dst-port="SampleIn"
        src="DeblockFiltVert" src-port="FiltSample"/>
    <Connection dst="DeblockFiltHoriz" dst-port="TilesCoord" src="" src-port="TilesCoord"/>
    <Connection dst="DeblockFiltHoriz" dst-port="LcuSizeMax" src="" src-port="LcuSizeMax"/>
    <Connection dst="DeblockFiltHoriz" dst-port="Bs" src="GenerateBs" src-port="BS"/>
    <Connection dst="" dst-port="FiltSample" src="DeblockFiltHoriz" src-port="FiltSample"/>
    <Connection dst="DeblockFiltHoriz" dst-port="PicSize" src="" src-port="PicSize"/>
    <Connection dst="DeblockFiltHoriz" dst-port="Qp" src="" src-port="Qp"/>
    <Connection dst="DeblockFiltHoriz" dst-port="IsPicSlcLcu" src="" src-port="IsPicSlcLcu"/>
    <Connection dst="DeblockFiltHoriz" dst-port="DBFDisable" src="" src-port="DBFDisable"/>
    <Connection dst="DeblockFiltHoriz" dst-port="SyntaxElem" src="" src-port="DbfSe"/>
</XDF>

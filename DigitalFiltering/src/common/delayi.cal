package common;

import common.constants.*;

actor delayi(int initial_sample) int(size=SAMPLE_SZ) operand_1 ==> int(size=SAMPLE_SZ) result :

	init: action ==> result:[ initial_sample ]
	end

	run: action operand_1:[ x ] ==> result:[ x ]
	end

	schedule fsm s_init:
		s_init	( init ) 	--> s_run;
		s_run	( run ) 	--> s_run;
	end

end

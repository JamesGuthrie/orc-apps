package stereo;
 
actor step() 
==>
uint(size=8) s1,
uint(size=8) s2:
 
	/*test: action ==> s:[N] repeat 4
	var
		List (type:uint(size=8) , size=7) N
	do
		foreach int i in 1 .. 4
		do
			N[i] := i*i;
		end 
	end*/
	
	action ==> s1:[n1],s2:[n2]
	var
		uint(size=8) n1, uint(size=8) n2 	
	do
		n1 := 1;
		n2 := 2;
	end	 
end

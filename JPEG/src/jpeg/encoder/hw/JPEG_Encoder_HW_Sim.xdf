<?xml version="1.0" encoding="UTF-8"?>
<XDF name="JPEG_Encoder_HW_Sim">
    <Instance id="Encoder">
        <Class name="jpeg.encoder.hw.encoder_write"/>
    </Instance>
    <Instance id="readYCbCr">
        <Class name="jpeg.encoder.common.YCbCrReader"/>
    </Instance>
    <Instance id="source">
        <Class name="org.ietr.stdio.Source"/>
    </Instance>
    <Instance id="writer">
        <Class name="jpeg.encoder.Writer"/>
    </Instance>
    <Instance id="readerBMP">
        <Class name="jpeg.encoder.converter.BmpReader"/>
    </Instance>
    <Connection dst="Encoder" dst-port="Y" src="readYCbCr" src-port="Y"/>

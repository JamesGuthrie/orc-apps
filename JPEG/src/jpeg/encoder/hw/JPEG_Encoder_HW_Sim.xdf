    <Connection dst="readYCbCr" dst-port="R" src="readerBMP" src-port="R"/>
    <Connection dst="readYCbCr" dst-port="G" src="readerBMP" src-port="G"/>
    <Connection dst="readYCbCr" dst-port="B" src="readerBMP" src-port="B"/>
    <Connection dst="Encoder" dst-port="SOI" src="readerBMP" src-port="SOI"/>
    <Connection dst="readYCbCr" dst-port="SOI" src="readerBMP" src-port="SOI"/>
</XDF>

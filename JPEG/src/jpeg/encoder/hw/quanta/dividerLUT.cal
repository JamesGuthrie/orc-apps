package jpeg.encoder.hw.quanta;

import jpeg.encoder.common.Tables.DivTab;

actor dividerLUT()
	int(size=32) num,
	int(size=32) den
		==>
			int(size=32) Out:
			
	action num:[n], den:[d] ==> Out:[o]
	var
		int o := 0
	do
		if n = 0 then
			o := 0;
		else
			if n < 0 then
				o := ((((n*(-1)) * DivTab[d - 1])) >> 16) * (-1);
			else
				o := ((n * DivTab[d - 1]) >> 16);
			end 
		end
	end
end
	
	
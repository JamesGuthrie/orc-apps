package avs.P;

actor Mgnt_InterPred_Luma()
	int(size=32)  PartSZ,
	int (size=32) MV,
	int(size=32) Location
		==>
	int (size=12) ReadAddr
		:

	ComputeReadAddr: action
			Location   :[ loc_x , loc_y ],
			PartSZ     :[ width , height ],
			MV         :[ mx, my ]
				==>
			ReadAddr :[ loc_x + (mx >> 2) - 2     , loc_y + (my >> 2) - 2     , width + 5     , height + 5 ]
		end
end
package sdf.impl;

actor p3_p4() ==> int O0_3, int O1_4 :

	int P0 = 3;
	int P1 = 4;

	List(type: int, size=P0) array0 := [i : for int i in 1 .. P0];
	List(type: int, size=P1) array1 := [i : for int i in 1 .. P1];

	action 
		==>
		O0_3:[array0] repeat P0,
		O1_4:[array1] repeat P1
	end

end

